generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Book {
  id     String  @id @default(cuid())
  author String
  title  String
  price  Int 

  @@unique([author, title])
}